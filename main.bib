@Article{	  kranton96,
  jstor_articletype={primary_article},
  title		= {Reciprocal Exchange: A Self-Sustaining System},
  author	= {Kranton, Rachel E.},
  journal	= {The American Economic Review},
  jstor_issuetitle={},
  volume	= {86},
  number	= {4},
  jstor_formatteddate={Sep., 1996},
  pages		= {830--851},
  url		= {http://www.jstor.org/stable/2118307},
  issn		= {00028282},
  abstract	= {Reciprocal exchange, or gift exchange, remains a
		  widespread means of obtaining goods and services. This
		  paper examines the persistence of reciprocal exchange by
		  formalizing the interaction between self-enforcing exchange
		  agreements and monetary market exchange. When more people
		  engage in reciprocal exchange, market search costs
		  increase, reciprocity is easier to enforce and yields
		  higher utility. Thus, personalized exchange can persist
		  even when it is inefficient. Conversely, large markets can
		  destroy reciprocity when reciprocal exchange is efficient.
		  The results characterize the use of personal "connections"
		  as a system of reciprocal exchange and explain the
		  disappearance of reciprocity when tribes encounter
		  markets.},
  publisher	= {American Economic Association},
  language	= {},
  copyright	= {Copyright  1996 American Economic Association},
  year		= {1996}
}

