@Article{	  ahuja-etal21,
  title		= {{IngID}: A framework for parsing and systematic reporting
		  of ingredients used in commercially packaged foods},
  journal	= {Journal of Food Composition and Analysis},
  volume	= {100},
  pages		= {103920},
  year		= {2021},
  issn		= {0889-1575},
  doi		= {https://doi.org/10.1016/j.jfca.2021.103920},
  url		= {https://www.sciencedirect.com/science/article/pii/S0889157521001204},
  author	= {Jaspreet K.C. Ahuja and Ying Li and Rahul Bahadur and
		  Quynhanh Nguyen and Ermias Haile and Pamela R. Pehrsson},
  keywords	= {Commercial ingredients, BFPD, IngID, Thesaurus, Food
		  ontology, Ingredient lists, Flour, Baked products,
		  Ingredient network and co-occurrence},
  abstract	= {There is lack of information in the scientific literature
		  on types of ingredients used in packaged foods. USDA’s
		  Global Branded Food Products Database makes publicly
		  available a compiled dataset of ingredient lists for over a
		  quarter million commercial food products. This paper
		  reports on the development of a framework for parsing and
		  systematic reporting of ingredients used in commercially
		  packaged foods (IngID) in the US and delineates the
		  complexity and challenges of current ingredient lists,
		  using baked products to illustrate. The major steps in the
		  development of IngID prototype were 1) identifying
		  top-selling baked products, 2) obtaining their ingredient
		  lists, 3) parsing individual ingredients after several
		  pre-processing steps as ingredient lists were inconsistent
		  and varied, 4) building a thesaurus by assigning a
		  preferred descriptor to equivalent terms such as synonyms
		  and spelling errors, and 5) assigning broader terms such as
		  flour, sweeteners. The current version of IngID includes 3
		  main files - an input Food details file, an output file of
		  parsed text strings, and a thesaurus of ∼6500 parsed
		  ingredients. IngID can help improve our understanding of
		  commercial ingredients, characterizing foods in dimensions
		  other than the traditional nutrient profiles, development
		  of food ontology, computer programs and artificial
		  intelligence tools.}
}

