@Article{	  angeletos07,
  title		= "Uninsured idiosyncratic investment risk and aggregate
		  saving",
  journal	= "Review of Economic Dynamics",
  volume	= "10",
  number	= "1",
  pages		= "1 - 30",
  year		= "2007",
  note		= "",
  issn		= "1094-2025",
  doi		= "DOI: 10.1016/j.red.2006.11.001",
  url		= "http://www.sciencedirect.com/science/article/B6WWT-4MR7DG4-1/2/321ab3b301256e6f17bf2b4003c7218d",
  author	= "George-Marios Angeletos",
  keywords	= "Precautionary saving",
  abstract	= "This paper augments the neoclassical growth model to study
		  the macroeconomic effects of uninsured idiosyncratic
		  investment, or capital-income, risk. Under standard
		  assumptions for preferences and technologies, individual
		  policy rules are linear in individual wealth, ensuring that
		  the equilibrium dynamics for aggregate quantities and
		  prices are independent of the wealth distribution. The
		  analysis thus remains highly tractable despite the
		  incompleteness of markets. As compared to complete markets,
		  the steady state is characterized by both a lower interest
		  rate and a lower capital stock when the elasticity of
		  intertemporal substitution is higher than the fraction of
		  private equity in total wealth. For empirically plausible
		  parameterizations, this condition is easily satisfied, and
		  the reduction in aggregate saving and income is
		  quantitatively significant. These findings contrast with
		  Bewley models, where idiosyncratic labor-income risk leads
		  to higher aggregate saving and income."
}

