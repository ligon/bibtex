@Article{	  barsky-etal86,
  jstor_articletype={research-article},
  title		= {Ricardian Consumers with Keynesian Propensities},
  author	= {Barsky, Robert B. and Mankiw, N. Gregory and Zeldes,
		  Stephen P.},
  journal	= {The American Economic Review},
  jstor_issuetitle={},
  volume	= {76},
  number	= {4},
  jstor_formatteddate={Sep., 1986},
  pages		= {676-691},
  url		= {http://www.jstor.org/stable/1806066},
  issn		= {00028282},
  abstract	= {This paper examines Ricardian equivalence in a world in
		  which taxes are not lump sum, but are levied on risky labor
		  income. It shows that the marginal propensity to consume
		  out of a tax cut, coupled with a future income tax
		  increase, can be substantial under plausible assumptions.
