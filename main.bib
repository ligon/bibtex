@Article{	  groves-ledyard77,
  jstor_articletype={research-article},
  title		= {Optimal Allocation of Public Goods: A Solution to the
		  "Free Rider" Problem},
  author	= {Groves, Theodore and Ledyard, John},
  journal	= {Econometrica},
  jstor_issuetitle={},
  volume	= {45},
  number	= {4},
  jstor_formatteddate={May, 1977},
  pages		= {pp. 783-809},
  url		= {http://www.jstor.org/stable/1912672},
  issn		= {00129682},
  abstract	= {This paper presents a general equilibrium model in which
		  private commodities are allocated through competitive
		  markets and public commodities according to government
		  allocation and taxing rules that depend on information
		  communicated to the government by consumers regarding their
		  preferences. A wide range of strategic behavior for
		  consumers in their communication with the government is
		  allowed; in particular, consumers may understate their
		  preferences and be "free riders" if they choose. Although
		  several examples of allocation-taxation schemes falling
		  within the general model are discussed, the major
		  contribution of the paper is the formulation of a
