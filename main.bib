@Unpublished{	  devereux06,
  title		= {Improved errors-in-variables estimators for grouped data},
  author	= {Devereux, Paul},
  year		= {2006},
  institution	= {School of Economics, University College Dublin},
  type		= {Working Papers},
  note		= "Manuscript",
  number	= {200602},
  abstract	= {Grouping models are widely used in economics but are
		  subject to finite sample bias. I show that the standard
		  errors-in-variables estimator (EVE) is exactly equivalent
		  to the Jackknife Instrumental Variables Estimator (JIVE),
		  and use this relationship to develop an estimator which,
		  unlike EVE, is unbiased in finite samples. The theoretical
		  results are demonstrated using Monte Carlo experiments.
		  Finally, I implement a model of intertemporal male labor
		  supply using microdata from the United States Census. There
		  are sizeable differences in the wage elasticity across
		  estimators, showing the practical importance of the
		  theoretical issues even when the sample size is quite
		  large.}
}

