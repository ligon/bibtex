@Article{	  case-paxson02,
  author	= "Case, Anne and Paxson, Christina",
  title		= "{Parental behavior and child health}",
  journal	= "Health Affairs",
  year		= "2002",
  volume	= "21",
  number	= "2",
  pages		= "164--178",
  note		= "March/April",
  read		= "nH",
  library	= "pu RA394.A1 H4",
  refdin	= "",
  keywords	= "",
  bib		= "",
  notes		= "",
  date		= "19020331",
  abstract	= " *'This paper documents the ways in which parental
		  behavior and socioeconomic status affect children's health.
		  Parental behavior in both the prenatal period and childhood
		  are examined, and evidence is presented on the correlation
		  of this behavior with income and parents' socioeconomic
		  status, and on the ways in which parents' actions affect
		  children's health. It is concluded that while health
		  insurance coverage and advances in medical treatment may be
		  important determinants of children's health, they cannot be
		  the only pillars: Protecting children's health also calls
		  for a broader set of policies that target parents'
		  health-related behavior.'"
}

